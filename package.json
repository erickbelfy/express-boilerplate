{
  "name": "express-boilerplate",
  "version": "0.0.1",
  "description": "simple stack to make it work",
  "main": "/src/index.js",
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=express-mongoose-es6-rest-api:* yarn start",
    "build": "gulp",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "commitmsg": "validate-commit-msg",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.4",
    "cz-conventional-changelog": "^2.0.0",
    "del": "^2.2.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.0.0",
    "gulp": "3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.3.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-util": "^3.0.8",
    "husky": "^0.13.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "run-sequence": "^1.2.2",
    "supertest": "^3.0.0",
    "validate-commit-msg": "^2.11.1"
  },
  "dependencies": {
    "bluebird": "3.4.7",
    "body-parser": "1.16.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "express-jwt": "^5.1.0",
    "express-validation": "^1.0.1",
    "express-winston": "^2.2.0",
    "helmet": "^3.4.1",
    "http-status": "^1.0.1",
    "joi": "^10.2.2",
    "jsonwebtoken": "^7.3.0",
    "method-override": "2.3.7",
    "mongoose": "4.8.5",
    "morgan": "^1.8.1",
    "winston": "^2.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
